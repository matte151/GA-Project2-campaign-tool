<div class="newOrbPartial">
    <form class="newOrbsBaseForm" action="/orbs" method="POST">
        <label>Name:</label><input type="text" name="name">
        <label>Class:</label><select name="class">
            <option name="Primary">Primary</option>
            <option name="Sub">Sub</option></select>
        <label>Dimension:</label><select name="dimension">
            <option name="Elemental">Elemental</option>
            <option name="Fabric">Fabric</option>
            <option name="Manipulation">Manipulation</option>
            <option name="Theoretical">Theoretical</option>
            <option name="Overall">Overall</option>
            <option name="Other">Other</option></select>
        <button type ="submit">Submit</button>
    </form>
</div>